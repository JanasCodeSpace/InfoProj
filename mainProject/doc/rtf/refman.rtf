00057 \par
00058     {\cf19 for} ({\cf18 size_t} i = 0; i < inputSize - windowSize; ++i)\par
00059     \{\par
00060         sumX = 0, sumY = 0, sumZ = 0;\par
00061         div = 0;\par
00062         p.setTime(it->getTime());\par
